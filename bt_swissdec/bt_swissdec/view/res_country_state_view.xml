<?xml version="1.0"?>
<openerp>
  <data>
    <!-- Add qst_ids to the State form -->
    <record model="ir.ui.view" id="view_country_state_form_ext">
      <field name="name">view_country_state_form_ext</field>
      <field name="model">res.country.state</field>
      <field name="inherit_id" ref="base.view_country_state_form"/>
      <field name="arch" type="xml">
        <field name="country_id" position="after">
        	<field name="qst_ids" colspan="4" nolabel="1"/>
        </field>
      </field>
    </record>
  </data>
</openerp>