<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Create Partner for Second Company -->
        <record id="second_partner" model="res.partner" context="{'default_is_company': True}">
            <field name="name">PROCESS AG</field>
            <field name="company_id" eval="None"/>
            <field name="is_company" eval="True"/>
            <field name="street"></field>
            <field name="city"></field>
            <field name="zip"></field>
            <field name="phone"></field>
            <field name="email">info@yourcompany.com</field>
            <field name="website">www.yourcompany.com</field>
            <field name="image_1920" type="base64" file="base/static/img/res_company_logo.png"/>
        </record>

        <!-- Create Second Company -->
        <record id="second_company" model="res.company">
            <field name="name">PROCESS AG</field>
            <field name="partner_id" ref="second_partner"/>
            <field name="currency_id" ref="base.CHF"/>
            <field name="salary_contact_name">Process Franz</field>
            <field name="salary_contact_email">franz@process.ch</field>
            <field name="salary_contact_phone">0313777777</field>
            <field name="street">Zelgstrasse 12</field>
            <field name="zip">3027</field>
            <field name="city">Bern</field>
            <field name="state_id" search="[('code','=','BE'), ('country_id.code','=','CH')]"/>
            <field name="country_id" search="[('code','=','CH')]" />
            <field name="currency_id" ref="base.CHF"/>
            <field eval="0" name="default_nettolohnausgleich"/>
            <field eval="0" name="default_zusatzmonatslohn"/>
            <field eval="1" name="default_booking_salary_payment"/>
            <field eval="1" name="company_used_for_bt_swissdec"/>
        </record>

        <record id="second_partner" model="res.partner">
            <field name="company_id" ref="second_company"/>
        </record>
    </data>
    <data>
